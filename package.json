{
  "name": "electron-compile-structure",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && rm -rf ./src/.cache",
    "app:Darwin-asar": "npm run clean && electron-packager-compile . --asar --version=1.2.7 --platform=darwin --arch=all --overwrite --out=build",
    "app:Darwin": "npm run clean && electron-packager-compile . --version=1.2.7 --platform=darwin --arch=all --overwrite --out=build",
    "start": "electron .",
    "asared": "export DEBUG=*,-babel && npm run app:Darwin-asar && open build/electron-compile-structure-darwin-x64/electron-compile-structure.app",
    "nonasared": "export DEBUG=*,-babel && npm run app:Darwin && open build/electron-compile-structure-darwin-x64/electron-compile-structure.app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnichols/electron-compile-structure.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mnichols/electron-compile-structure/issues"
  },
  "homepage": "https://github.com/mnichols/electron-compile-structure#readme",
  "devDependencies": {
    "asar": "^0.11.0",
    "electron-compile": "^3.3.5",
    "electron-compilers": "^3.1.6",
    "electron-packager": "^7.3.0",
    "electron-prebuilt-compile": "^1.2.7"
  }
}
